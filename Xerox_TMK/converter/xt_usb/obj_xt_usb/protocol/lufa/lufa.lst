 107:../../tmk_core/protocol/lufa/lufa.c **** // TODO: Around 2500ms delay often works anyhoo but proper startup would be better
 108:../../tmk_core/protocol/lufa/lufa.c **** // 1000ms delay of hid_listen affects this probably
 109:../../tmk_core/protocol/lufa/lufa.c **** /* wait for Console startup */
 110:../../tmk_core/protocol/lufa/lufa.c **** static bool console_is_ready(void)
 111:../../tmk_core/protocol/lufa/lufa.c **** {
 112:../../tmk_core/protocol/lufa/lufa.c ****     static bool hid_listen_ready = false;
 113:../../tmk_core/protocol/lufa/lufa.c ****     if (!hid_listen_ready) {
 114:../../tmk_core/protocol/lufa/lufa.c ****         if (timer_read32() < 2500)
 115:../../tmk_core/protocol/lufa/lufa.c ****             return false;
 116:../../tmk_core/protocol/lufa/lufa.c ****         hid_listen_ready = true;
 117:../../tmk_core/protocol/lufa/lufa.c ****     }
 118:../../tmk_core/protocol/lufa/lufa.c ****     return true;
 119:../../tmk_core/protocol/lufa/lufa.c **** }
 120:../../tmk_core/protocol/lufa/lufa.c **** 
 121:../../tmk_core/protocol/lufa/lufa.c **** static bool console_putc(uint8_t c)
 122:../../tmk_core/protocol/lufa/lufa.c **** {
 123:../../tmk_core/protocol/lufa/lufa.c ****     // return immediately if called while interrupt
 124:../../tmk_core/protocol/lufa/lufa.c ****     if (!(SREG & (1<<SREG_I)))
 125:../../tmk_core/protocol/lufa/lufa.c ****         goto EXIT;
 126:../../tmk_core/protocol/lufa/lufa.c **** 
 127:../../tmk_core/protocol/lufa/lufa.c ****     if (USB_DeviceState != DEVICE_STATE_Configured && !ringbuf_is_full(&sendbuf))
 128:../../tmk_core/protocol/lufa/lufa.c ****         goto EXIT;
 129:../../tmk_core/protocol/lufa/lufa.c **** 
 130:../../tmk_core/protocol/lufa/lufa.c ****     if (!console_is_ready() && !ringbuf_is_full(&sendbuf))
 131:../../tmk_core/protocol/lufa/lufa.c ****         goto EXIT;
 132:../../tmk_core/protocol/lufa/lufa.c **** 
 133:../../tmk_core/protocol/lufa/lufa.c ****     /* Data lost considerations:
 134:../../tmk_core/protocol/lufa/lufa.c ****      * 1. When buffer is full at early satage of startup, we will have to start sending
 135:../../tmk_core/protocol/lufa/lufa.c ****      * before console_is_ready() returns true. Data can be lost even if sending data
 136:../../tmk_core/protocol/lufa/lufa.c ****      * seems to be successful on USB. hid_listen on host is not ready perhaps?
 137:../../tmk_core/protocol/lufa/lufa.c ****      * Sometime first few packets are lost when buffer is full at startup.
 138:../../tmk_core/protocol/lufa/lufa.c ****      * 2. When buffer is full and USB pipe is not ready, new coming data will be lost.
 139:../../tmk_core/protocol/lufa/lufa.c ****      * 3. console_task() cannot send data in buffer while main loop is blocked.
 140:../../tmk_core/protocol/lufa/lufa.c ****      */
 141:../../tmk_core/protocol/lufa/lufa.c ****     /* retry timeout */
 142:../../tmk_core/protocol/lufa/lufa.c ****     const uint8_t CONSOLE_TIMOUT = 5;   // 1 is too small, 2 seems to be enough for Linux
 143:../../tmk_core/protocol/lufa/lufa.c ****     static uint8_t timeout = CONSOLE_TIMOUT;
 144:../../tmk_core/protocol/lufa/lufa.c ****     uint16_t prev = timer_read();
 145:../../tmk_core/protocol/lufa/lufa.c ****     bool done = false;
 146:../../tmk_core/protocol/lufa/lufa.c **** 
 147:../../tmk_core/protocol/lufa/lufa.c ****     uint8_t ep = Endpoint_GetCurrentEndpoint();
 148:../../tmk_core/protocol/lufa/lufa.c ****     Endpoint_SelectEndpoint(CONSOLE_IN_EPNUM);
 149:../../tmk_core/protocol/lufa/lufa.c **** 
 150:../../tmk_core/protocol/lufa/lufa.c **** AGAIN:
 151:../../tmk_core/protocol/lufa/lufa.c ****     if (Endpoint_IsStalled() || !Endpoint_IsEnabled() || !Endpoint_IsConfigured()) {
 152:../../tmk_core/protocol/lufa/lufa.c ****         goto EXIT_RESTORE_EP;
 153:../../tmk_core/protocol/lufa/lufa.c ****     }
 154:../../tmk_core/protocol/lufa/lufa.c **** 
 155:../../tmk_core/protocol/lufa/lufa.c ****     // write from buffer to endpoint bank
 156:../../tmk_core/protocol/lufa/lufa.c ****     while (!ringbuf_is_empty(&sendbuf) && Endpoint_IsReadWriteAllowed()) {
 157:../../tmk_core/protocol/lufa/lufa.c ****         Endpoint_Write_8(ringbuf_get(&sendbuf));
 158:../../tmk_core/protocol/lufa/lufa.c **** 
 159:../../tmk_core/protocol/lufa/lufa.c ****         // clear bank when it is full
 160:../../tmk_core/protocol/lufa/lufa.c ****         if (!Endpoint_IsReadWriteAllowed() && Endpoint_IsINReady()) {
 161:../../tmk_core/protocol/lufa/lufa.c ****             Endpoint_ClearIN();
 162:../../tmk_core/protocol/lufa/lufa.c ****             timeout = CONSOLE_TIMOUT; // re-enable retry only when host can receive
 163:../../tmk_core/protocol/lufa/lufa.c ****         }
 164:../../tmk_core/protocol/lufa/lufa.c ****     }
 165:../../tmk_core/protocol/lufa/lufa.c **** 
 166:../../tmk_core/protocol/lufa/lufa.c ****     // write c to bank directly if there is no others in buffer
 167:../../tmk_core/protocol/lufa/lufa.c ****     if (ringbuf_is_empty(&sendbuf) && Endpoint_IsReadWriteAllowed()) {
 168:../../tmk_core/protocol/lufa/lufa.c ****         Endpoint_Write_8(c);
 169:../../tmk_core/protocol/lufa/lufa.c ****         done = true;
 170:../../tmk_core/protocol/lufa/lufa.c ****     }
 171:../../tmk_core/protocol/lufa/lufa.c **** 
 172:../../tmk_core/protocol/lufa/lufa.c ****     // clear bank when there are chars in bank
 173:../../tmk_core/protocol/lufa/lufa.c ****     if (Endpoint_BytesInEndpoint() && Endpoint_IsINReady()) {
 174:../../tmk_core/protocol/lufa/lufa.c ****         // Windows needs to fill packet with 0
 175:../../tmk_core/protocol/lufa/lufa.c ****         while (Endpoint_IsReadWriteAllowed()) {
 176:../../tmk_core/protocol/lufa/lufa.c ****                 Endpoint_Write_8(0);
 177:../../tmk_core/protocol/lufa/lufa.c ****         }
 178:../../tmk_core/protocol/lufa/lufa.c ****         Endpoint_ClearIN();
 179:../../tmk_core/protocol/lufa/lufa.c ****         timeout = CONSOLE_TIMOUT; // re-enable retry only when host can receive
 180:../../tmk_core/protocol/lufa/lufa.c ****     }
 181:../../tmk_core/protocol/lufa/lufa.c **** 
 182:../../tmk_core/protocol/lufa/lufa.c ****     if (done) {
 183:../../tmk_core/protocol/lufa/lufa.c ****         Endpoint_SelectEndpoint(ep);
 184:../../tmk_core/protocol/lufa/lufa.c ****         return true;
 185:../../tmk_core/protocol/lufa/lufa.c ****     }
 186:../../tmk_core/protocol/lufa/lufa.c **** 
 187:../../tmk_core/protocol/lufa/lufa.c ****     /* retry when buffer is full.
 188:../../tmk_core/protocol/lufa/lufa.c ****      * once timeout this is disabled until host receives actually,
 189:../../tmk_core/protocol/lufa/lufa.c ****      * otherwise this will block or make main loop execution sluggish.
 190:../../tmk_core/protocol/lufa/lufa.c ****      */
 191:../../tmk_core/protocol/lufa/lufa.c ****     if (ringbuf_is_full(&sendbuf) && timeout) {
 192:../../tmk_core/protocol/lufa/lufa.c ****         uint16_t curr = timer_read();
 193:../../tmk_core/protocol/lufa/lufa.c ****         if (curr != prev) {
 194:../../tmk_core/protocol/lufa/lufa.c ****             timeout--;
 195:../../tmk_core/protocol/lufa/lufa.c ****             prev = curr;
 196:../../tmk_core/protocol/lufa/lufa.c ****         }
 197:../../tmk_core/protocol/lufa/lufa.c ****         goto AGAIN;
 198:../../tmk_core/protocol/lufa/lufa.c ****     }
 199:../../tmk_core/protocol/lufa/lufa.c **** 
 200:../../tmk_core/protocol/lufa/lufa.c **** EXIT_RESTORE_EP:
 201:../../tmk_core/protocol/lufa/lufa.c ****     Endpoint_SelectEndpoint(ep);
 202:../../tmk_core/protocol/lufa/lufa.c **** EXIT:
 203:../../tmk_core/protocol/lufa/lufa.c ****     return ringbuf_put(&sendbuf, c);
 204:../../tmk_core/protocol/lufa/lufa.c **** }
 205:../../tmk_core/protocol/lufa/lufa.c **** 
 206:../../tmk_core/protocol/lufa/lufa.c **** static void console_flush(void)
 207:../../tmk_core/protocol/lufa/lufa.c **** {
 208:../../tmk_core/protocol/lufa/lufa.c ****     if (!console_is_ready())
 209:../../tmk_core/protocol/lufa/lufa.c ****         return;
 210:../../tmk_core/protocol/lufa/lufa.c **** 
 211:../../tmk_core/protocol/lufa/lufa.c ****     if (USB_DeviceState != DEVICE_STATE_Configured)
 212:../../tmk_core/protocol/lufa/lufa.c ****         return;
 213:../../tmk_core/protocol/lufa/lufa.c **** 
 214:../../tmk_core/protocol/lufa/lufa.c ****     uint8_t ep = Endpoint_GetCurrentEndpoint();
 215:../../tmk_core/protocol/lufa/lufa.c **** 
 216:../../tmk_core/protocol/lufa/lufa.c ****     Endpoint_SelectEndpoint(CONSOLE_IN_EPNUM);
 217:../../tmk_core/protocol/lufa/lufa.c ****     if (!Endpoint_IsEnabled() || !Endpoint_IsConfigured()) {
 218:../../tmk_core/protocol/lufa/lufa.c ****         Endpoint_SelectEndpoint(ep);
 219:../../tmk_core/protocol/lufa/lufa.c ****         return;
 220:../../tmk_core/protocol/lufa/lufa.c ****     }
 221:../../tmk_core/protocol/lufa/lufa.c **** 
 222:../../tmk_core/protocol/lufa/lufa.c ****     // write from buffer to endpoint bank
 223:../../tmk_core/protocol/lufa/lufa.c ****     while (!ringbuf_is_empty(&sendbuf) && Endpoint_IsReadWriteAllowed()) {
 224:../../tmk_core/protocol/lufa/lufa.c ****         Endpoint_Write_8(ringbuf_get(&sendbuf));
 225:../../tmk_core/protocol/lufa/lufa.c **** 
 226:../../tmk_core/protocol/lufa/lufa.c ****         // clear bank when it is full
 227:../../tmk_core/protocol/lufa/lufa.c ****         if (!Endpoint_IsReadWriteAllowed() && Endpoint_IsINReady()) {
 228:../../tmk_core/protocol/lufa/lufa.c ****             Endpoint_ClearIN();
 229:../../tmk_core/protocol/lufa/lufa.c ****         }
 230:../../tmk_core/protocol/lufa/lufa.c ****     }
 231:../../tmk_core/protocol/lufa/lufa.c **** 
 232:../../tmk_core/protocol/lufa/lufa.c ****     // clear bank when there are chars in bank
 233:../../tmk_core/protocol/lufa/lufa.c ****     if (Endpoint_BytesInEndpoint() && Endpoint_IsINReady()) {
 234:../../tmk_core/protocol/lufa/lufa.c ****         // Windows needs to fill packet with 0
 235:../../tmk_core/protocol/lufa/lufa.c ****         while (Endpoint_IsReadWriteAllowed()) {
 236:../../tmk_core/protocol/lufa/lufa.c ****                 Endpoint_Write_8(0);
 237:../../tmk_core/protocol/lufa/lufa.c ****         }
 238:../../tmk_core/protocol/lufa/lufa.c ****         Endpoint_ClearIN();
 239:../../tmk_core/protocol/lufa/lufa.c ****     }
 240:../../tmk_core/protocol/lufa/lufa.c **** 
 241:../../tmk_core/protocol/lufa/lufa.c ****     Endpoint_SelectEndpoint(ep);
 242:../../tmk_core/protocol/lufa/lufa.c **** }
 243:../../tmk_core/protocol/lufa/lufa.c **** 
 244:../../tmk_core/protocol/lufa/lufa.c **** static void console_task(void)
 245:../../tmk_core/protocol/lufa/lufa.c **** {
 246:../../tmk_core/protocol/lufa/lufa.c ****     static uint16_t fn = 0;
 247:../../tmk_core/protocol/lufa/lufa.c ****     if (fn == USB_Device_GetFrameNumber()) {
 248:../../tmk_core/protocol/lufa/lufa.c ****         return;
 249:../../tmk_core/protocol/lufa/lufa.c ****     }
 250:../../tmk_core/protocol/lufa/lufa.c ****     fn = USB_Device_GetFrameNumber();
 251:../../tmk_core/protocol/lufa/lufa.c ****     console_flush();
 252:../../tmk_core/protocol/lufa/lufa.c **** }
