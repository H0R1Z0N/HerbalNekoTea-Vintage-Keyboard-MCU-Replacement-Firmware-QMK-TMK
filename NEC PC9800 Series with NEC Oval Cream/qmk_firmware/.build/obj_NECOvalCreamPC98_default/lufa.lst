 178:tmk_core/protocol/lufa/lufa.c **** /** \brief Raw HID Send
 179:tmk_core/protocol/lufa/lufa.c ****  *
 180:tmk_core/protocol/lufa/lufa.c ****  * FIXME: Needs doc
 181:tmk_core/protocol/lufa/lufa.c ****  */
 182:tmk_core/protocol/lufa/lufa.c **** void raw_hid_send(uint8_t *data, uint8_t length) {
 183:tmk_core/protocol/lufa/lufa.c ****     // TODO: implement variable size packet
 184:tmk_core/protocol/lufa/lufa.c ****     if (length != RAW_EPSIZE) {
 185:tmk_core/protocol/lufa/lufa.c ****         return;
 186:tmk_core/protocol/lufa/lufa.c ****     }
 187:tmk_core/protocol/lufa/lufa.c **** 
 188:tmk_core/protocol/lufa/lufa.c ****     if (USB_DeviceState != DEVICE_STATE_Configured) {
 189:tmk_core/protocol/lufa/lufa.c ****         return;
 190:tmk_core/protocol/lufa/lufa.c ****     }
 191:tmk_core/protocol/lufa/lufa.c **** 
 192:tmk_core/protocol/lufa/lufa.c ****     // TODO: decide if we allow calls to raw_hid_send() in the middle
 193:tmk_core/protocol/lufa/lufa.c ****     // of other endpoint usage.
 194:tmk_core/protocol/lufa/lufa.c ****     uint8_t ep = Endpoint_GetCurrentEndpoint();
 195:tmk_core/protocol/lufa/lufa.c **** 
 196:tmk_core/protocol/lufa/lufa.c ****     Endpoint_SelectEndpoint(RAW_IN_EPNUM);
 197:tmk_core/protocol/lufa/lufa.c **** 
 198:tmk_core/protocol/lufa/lufa.c ****     // Check to see if the host is ready to accept another packet
 199:tmk_core/protocol/lufa/lufa.c ****     if (Endpoint_IsINReady()) {
 200:tmk_core/protocol/lufa/lufa.c ****         // Write data
 201:tmk_core/protocol/lufa/lufa.c ****         Endpoint_Write_Stream_LE(data, RAW_EPSIZE, NULL);
 202:tmk_core/protocol/lufa/lufa.c ****         // Finalize the stream transfer to send the last packet
 203:tmk_core/protocol/lufa/lufa.c ****         Endpoint_ClearIN();
 204:tmk_core/protocol/lufa/lufa.c ****     }
 205:tmk_core/protocol/lufa/lufa.c **** 
 206:tmk_core/protocol/lufa/lufa.c ****     Endpoint_SelectEndpoint(ep);
 207:tmk_core/protocol/lufa/lufa.c **** }
 208:tmk_core/protocol/lufa/lufa.c **** 
 209:tmk_core/protocol/lufa/lufa.c **** /** \brief Raw HID Receive
 210:tmk_core/protocol/lufa/lufa.c ****  *
 211:tmk_core/protocol/lufa/lufa.c ****  * FIXME: Needs doc
 212:tmk_core/protocol/lufa/lufa.c ****  */
 213:tmk_core/protocol/lufa/lufa.c **** __attribute__((weak)) void raw_hid_receive(uint8_t *data, uint8_t length) {
 214:tmk_core/protocol/lufa/lufa.c ****     // Users should #include "raw_hid.h" in their own code
 215:tmk_core/protocol/lufa/lufa.c ****     // and implement this function there. Leave this as weak linkage
 216:tmk_core/protocol/lufa/lufa.c ****     // so users can opt to not handle data coming in.
 217:tmk_core/protocol/lufa/lufa.c **** }
 218:tmk_core/protocol/lufa/lufa.c **** 
 219:tmk_core/protocol/lufa/lufa.c **** /** \brief Raw HID Task
 220:tmk_core/protocol/lufa/lufa.c ****  *
 221:tmk_core/protocol/lufa/lufa.c ****  * FIXME: Needs doc
 222:tmk_core/protocol/lufa/lufa.c ****  */
 223:tmk_core/protocol/lufa/lufa.c **** static void raw_hid_task(void) {
 224:tmk_core/protocol/lufa/lufa.c ****     // Create a temporary buffer to hold the read in data from the host
 225:tmk_core/protocol/lufa/lufa.c ****     uint8_t data[RAW_EPSIZE];
 226:tmk_core/protocol/lufa/lufa.c ****     bool    data_read = false;
 227:tmk_core/protocol/lufa/lufa.c **** 
 228:tmk_core/protocol/lufa/lufa.c ****     // Device must be connected and configured for the task to run
 229:tmk_core/protocol/lufa/lufa.c ****     if (USB_DeviceState != DEVICE_STATE_Configured) return;
 230:tmk_core/protocol/lufa/lufa.c **** 
 231:tmk_core/protocol/lufa/lufa.c ****     Endpoint_SelectEndpoint(RAW_OUT_EPNUM);
 232:tmk_core/protocol/lufa/lufa.c **** 
 233:tmk_core/protocol/lufa/lufa.c ****     // Check to see if a packet has been sent from the host
 234:tmk_core/protocol/lufa/lufa.c ****     if (Endpoint_IsOUTReceived()) {
 235:tmk_core/protocol/lufa/lufa.c ****         // Check to see if the packet contains data
 236:tmk_core/protocol/lufa/lufa.c ****         if (Endpoint_IsReadWriteAllowed()) {
 237:tmk_core/protocol/lufa/lufa.c ****             /* Read data */
 238:tmk_core/protocol/lufa/lufa.c ****             Endpoint_Read_Stream_LE(data, sizeof(data), NULL);
 239:tmk_core/protocol/lufa/lufa.c ****             data_read = true;
 240:tmk_core/protocol/lufa/lufa.c ****         }
 241:tmk_core/protocol/lufa/lufa.c **** 
 242:tmk_core/protocol/lufa/lufa.c ****         // Finalize the stream transfer to receive the last packet
 243:tmk_core/protocol/lufa/lufa.c ****         Endpoint_ClearOUT();
 244:tmk_core/protocol/lufa/lufa.c **** 
 245:tmk_core/protocol/lufa/lufa.c ****         if (data_read) {
 246:tmk_core/protocol/lufa/lufa.c ****             raw_hid_receive(data, sizeof(data));
 247:tmk_core/protocol/lufa/lufa.c ****         }
 248:tmk_core/protocol/lufa/lufa.c ****     }
 249:tmk_core/protocol/lufa/lufa.c **** }
 250:tmk_core/protocol/lufa/lufa.c **** #endif
